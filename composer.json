{
    "name": "drupol/yaroc",
    "type": "library",
    "description": "YAROC: Yet Another Random.Org Client.",
    "keywords": [
        "random",
        "client",
        "random.org",
        "RNG"
    ],
    "homepage": "https://github.com/drupol/yaroc",
    "license": "MIT",
    "authors": [
        {
            "name": "Pol Dellaiera",
            "email": "pol.dellaiera@protonmail.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">= 7.4",
        "ext-json": "*",
        "symfony/http-client-contracts": "^2"
    },
    "require-dev": {
        "drupol/php-conventions": "^3",
        "friends-of-phpspec/phpspec-code-coverage": "^6.1",
        "infection/infection": "^0.21",
        "infection/phpspec-adapter": "^0.1.2",
        "ircmaxell/random-lib": "^1.2",
        "phpspec/phpspec": "^7",
        "rych/random": "^0.1.0",
        "symfony/http-client": "^4.4 || ^5.2",
        "vimeo/psalm": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "drupol\\Yaroc\\": "src/",
            "drupol\\Yaroc\\Examples\\": "examples/"
        }
    },
    "support": {
        "issues": "https://github.com/drupol/yaroc/issues",
        "source": "https://github.com/drupol/yaroc"
    }
}
